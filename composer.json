{
    "name": "encore/lumen-swoole",
    "type": "library",
    "description": "lumen-swoole can help you work with lumen and swoole.",
    "keywords": ["lumen", "swoole", "performance", "framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "z-song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.5.*",
        "ext-swoole" : ">=1.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/lumen": "5.5.*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Encore\\LumenSwoole\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "bin": [
        "bin/lumen-swoole"
    ]
}
